#!/bin/bash

# Only reply to "get" requests, not store or others
[[ "$1" == "get" ]] || exit

if [[ -z "$GITHUB_BEW_PASSWORD" ]]; then
    echo "Missing env var GITHUB_BEW_PASSWORD" >&2
    exit 1
fi

echo "### Using Github bew password from environment" >&2

echo "username=bew"
echo "password=$GITHUB_BEW_PASSWORD"
